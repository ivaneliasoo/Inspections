{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Exceptions",
      "Serilog",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss.fff zzz} [{Level}] [{SourceContext}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://seq:5341",
          "apiKey": "none",
          "restrictedToMinimumLevel": "Verbose"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Inspections": "Host=localhost;Port=5432;Username=appuser;Password=appuser;Database=inspections;"
  },
  "AWS": {
    "Profile": "default",
    "Region": "ap-southeast-1"
  },
  "ClientSettings": {
    "S3BucketName": "cse-inspetions-test-photos",
    "ReportsImagesFolder": "MediaUpload/img/Reports",
    "TempFolder": "TempFolder"
  },
  "JwtSettings": {
    "Issuer": "https://localhost:5001/",
    "Audience": "https://localhost:5001/",
    "SecretKey": "e5df4ae6-b61f-4df9-98c7-8c4dafbb2571",
    "ExpirationHours": 8640
  }
}
